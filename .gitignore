# Database config and secrets
/config/database.yml
/config/secrets.yml

# Ignore bundler config
/.bundle

# Ignore client credentials
/config/client_api_credentials.yml

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/db/structure.sql
/doc/app/*
/vendor/cldr/*
/public/uploads/*
/public/photo/*
/public/test/*
/test/assets/*
/spec/assets/*
/public/assets/**
.powenv
.rvmrc
.env
.ruby-version

